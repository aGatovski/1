function judge(array) 
{
  
    for (let i = 0; i < array.length/2; i++) {
    swap(array, i, array.length - 1 - i);
    }
    console.log(array.join(' '));
    function swap(elements, i, j) {
    let temp = elements[i];
    elements[i] = elements[j];
    elements[j] = temp;
    }
   
}

judge([1,2,3,4,5,6,7,8,9])
